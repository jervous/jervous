<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title></title>
    <link rel="stylesheet" href="dist/css/bootstrap.min.css">
    <script src="javascripts/jquery-3.0.0.js"></script>
    <script src="dist/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="stylesheets/style.css">
</head>
<body>
<%- include header %>
<div style="width:1024px;margin:1em auto;">
    <div style="border:1px solid green;width:720px;float:left">
        <ul class="horul">
            <li>最新</li>
            <li>最热</li>
            <li>未回答</li>
        </ul>
        <table class="table">
            <tbody class="answer">
            <% rs.forEach(function(row){ %>
            <tr>
                <td>
                    <div><%=row['looknum']%><br/>回答</div>
                </td>
                <td><%=row['renum']%><br/>浏览</td>
                <td><%=row['title']%></td>
            </tr>
            <% }) %>
            </tbody>
        </table>
    </div>
    <div style="border:1px solid green;width:300px;float:right">
        <table class="table">
            <caption>排行榜</caption>
            <tbody>
            <tr>
                <td>1</td>
                <td>Bangalore</td>
            </tr>
            <tr>
                <td>2</td>
                <td>Mumbai</td>
            </tr>
            <tr>
                <td>3</td>
                <td>Bangalore</td>
            </tr>
            </tbody>
        </table>
    </div>

    <!--登录注册-->
    <div class="modal fade" id="loginModal">
        <div class="modal-dialog">
            <div class="modal-content">

            </div>
        </div>
    </div>
</div>
</body>
</html>
